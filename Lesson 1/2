#include<stdio.h>
#include<stdlib.h>
int main()
{
float x1,y1,x2,y2,x3,y3;
printf("\n Enter the coordinate of (x1,y1):");
scanf("%f,%f",&x1,&y1);
printf("\n Enter the coordinates of(x2,y2)");
scanf("%f,%f", &x2,&y2);
printf("\n Enter the coordinates of (x3,y3)");
scanf("%f,%f",&x3,&y3);
if((y2-y1)/(x2-x1)==(y3-y1)/(x3-x1))
{
    printf("points lies on the straight line");
}
else
printf(" the points are not lies on straight line");
     return 0;
}
