#include<stdio.h>

int main(){
int s1,s2,s3,s4,s5;
float total,percentage;
printf("Enter mark in s1: \n");
scanf("%d",&s1);
printf("Enter mark in s2: \n");
scanf("%d",&s2);
printf("Enter mark in s3: \n");
scanf("%d",&s3);
printf("Enter mark in s4: \n");
scanf("%d",&s4);
printf("Enter mark in s5: \n");
scanf("%d",&s5);
total=s1+s2+s3+s4+s5;
percentage=(total/500)*100;
printf("The total marks are : %f\n",total);
printf("The percentage is : %f\n",percentage);
return 0;
}
