#include<stdio.h>

int main(){
 int num, sum, i,number,count=0,n=1;
 printf("Enter N digits Number:");
 scanf("%d", &num);
 number = num;
 while(number!=0)
 {
    number = number/10;
    count = count +1;
 }
 for(i=1;i<count;i++)
 {
    n= n*10;
    n= n+1;
 }
 sum = num + n;
 printf("output: %d",sum);
    return 0;
}
