#include<stdio.h>
int main(){
   float selling_price,cost_price, income;
   printf("Enter the the value of cost price:");
   scanf("%f",&cost_price);
   printf("Enter the value of selling price:");
   scanf("%f",&selling_price);
   income = selling_price - cost_price;
   if(income>0){
    printf("income is :%f\n",income);
   }
    else if(income<0){
    printf("Loss is: %.1f\n",income);
   }
   else if(income==0){
    printf("The overall income is Zero");
   }
    return 0;
}
