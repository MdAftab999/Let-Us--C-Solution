#include<stdio.h>
int main()
{
float hours;
printf("Enter the number of hours workers taken to finish the job\n");
scanf("%f",&hours);
if(hours>=2&&hours<=3)
printf("Worker is highly efficient\n");
else
{
    if(hours>=3&&hours<=4)
    printf("\n Worker is ordered to improve his efficiency");
    else
    {
        if(hours>=4&&hours<=5)
        printf("\n Worker should be given training to improve his efficiency");
        else
        printf("\n worker should leave the company");
    }
}
printf("\nPress any key to exit");

    
    return 0;
