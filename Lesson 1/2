#include<stdio.h>

int main(){
    int c,d,e;
    printf("\n Enter the location of C:");
    scanf("%d", &c);
    printf("\n Enter the location of D :");
    scanf("%d", &d);
    e=c;
    c=d;
    d=e;
    printf("The new location of C is :%d\n", c);
    printf("The new location of D is : %d\n", d);
    return 0;
}
