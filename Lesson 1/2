#include<stdio.h>
#include<stdlib.h>
int main()
{
int rac_b,rac_l,area_rac,peri_rac;
printf("\n Enter the lenght of ractangle:  ");
scanf("%d", &rac_l);
printf("\n Enter of the breadth of Ractangle: ");
scanf("%d",&rac_b);
area_rac=rac_l*rac_b;
peri_rac=2*(rac_l+rac_b);
if(area_rac>peri_rac)
{
    printf("\nThe Area of ractangle %d is greater than Perimeter of ractangle %d",area_rac,peri_rac);
}
else
printf("\nThe Perimeter of ractangle %d is greater than Area of Rectangle %d",area_rac,peri_rac);
     return 0;
}
