#include<stdio.h>
int main()
{
    char ch;
    printf(" Enter any character :\n");
    scanf("%c",&ch);
    if(ch>=65 && ch<=90)
    {
        printf("Input character is capital letter :\n");
    }
    else if(ch>=97 && ch<=122)
    {
        printf("Input character is small letter:\n");
    }
    else if(ch>=48 && ch<=57)
    {
        printf("Input character is digit\n:");
    }
    else
    {
        printf("Input character is symblos:\n");
    }
     return 0;
}
