#include<stdio.h>
int main()
{
    int a,b,c, sum, largest;
    printf(" Enter the three side of triangle\n");
    scanf("%d %d %d",&a, &b, &c);
    if(a>b && a>c)
    {
        sum=b+c;
        largest=a;
    }
    else if(b>a && b>c)
    {
    sum=a+c;
    largest= b;
    }
    else
    {
        sum= a+b;
        largest=c;
    }
    printf("\n largest side is %d",largest,sum);
    if(sum>largest)
    {
        printf("\n the triagle is valid");
    }
    else
    {
        printf("\n the triagle is not valid");
    }
    return 0;
}
f
#include<stdio.h>
int main()
{
    int a,b,c, sum, largest;
    printf(" Enter the three side of triangle\n");
    scanf("%d %d %d",&a, &b, &c);
    if(a==b && b==c &&c==a)
    {
        printf("\nThe triangle is equilateral");
    }
    if((a==b && b!=c)|| (b==c &&a!=c)||(c==a&&a!=b))
    {
        printf("\n The triagle is isosceles");
    }
    if(a!=b && b!=c && c!=a)
    {
        printf("\nThe triangle is scalene");
    }
    if((a*a==b*b+c*c)||(b*b==a*a+c*c)||(c*c==a*a+b*b))
    {
        printf("\nThe triangle is right ");
    }
    
    return 0;
}
