#include<stdio.h>
int main()
{
     int year;
     printf("\nEnter the Year:\n");
     scanf("%d",&year);
     ((year%4==0 && year%100!=0)?printf("Leap year"):((year%400==0)?printf("leap y"):printf("Not leap year")));
return 0;
}
