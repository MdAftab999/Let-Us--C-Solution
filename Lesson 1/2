#include<stdio.h>
int main(){
int salary;
float gross_salary;
printf("Enter basic Salary of Ramesh");
scanf("%d",&salary);
gross_salary = salary+0.4*salary+0.2*salary;
printf("The gross_salary of Ramesh is: %f",gross_salary);
return 0;
}
Q2-
#include<stdio.h>

int main(){
float distance,m,f,i,cm;
printf("Enter the distance between two cities in KM:");
scanf("%f",&distance);
//1km=1000m
//1km=3280.84 feet
//1km=39370.1 inches
//1km=100000cm
m = 1000*distance;
f = 3280.84*distance;
i = 39370.1*distance;
cm = 100000*distance;
printf("The distance in meter is : %f\n", m);
printf("The distance in feet is : %f\n", f);
printf("The distance in inches is : %f\n", i);
printf("The distance in centimetre is : %f\n", cm);
return 0;
}
