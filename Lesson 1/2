#include<stdio.h>

int main(){
int num,d1,d2,d3,d4,d5,sum;
printf("Enter the number");
scanf("%d", &num);
d1 = num/10000;
d2 = (num-10000*d1)/1000;
d3 = (num-10000*d1-1000*d2)/100;
d4 = (num-10000*d1-1000*d2-100*d3)/10;
d5 = num%10;
printf("The digit at 10- thousand position is:  %d\n",d1);
printf("The digit at 10- thousand position is:  %d\n",d2);
printf("The digit at 10- thousand position is:  %d\n",d3);
printf("The digit at 10- thousand position is:  %d\n",d4);
printf("The digit at 10- thousand position is:  %d\n",d5);
sum = d1+d2+d3+d4+d5;
printf("The sum of the digits of the number you entered is: %d", sum);
return 0;
}
