#include<stdio.h>
int main()
{
    int ram, shyam, ajay;
    printf("Enter the age of Ram: \n");
    scanf("%d",&ram);
    printf("Enter the age of shyam: \n");
    scanf("%d",&shyam);
    printf("Enter the age of Ajay : \n");
    scanf("%d",&ajay);
    if(ram<shyam)
    {
        if(ram<ajay)
        {
            printf("Ram is youngest");
        }
        else
        {
            printf("Ajay is the youngest");
        }
    }
    else if(shyam<ajay)
    {
        printf("Shyam is youngest");
    }
    else
    {
        printf("Ajay is the youngest");
    }
    return 0;
}
F
#include<stdio.h>
int main()
{
    int ram, shyam, ajay;
    printf("Enter the age of Ram :\n");
    scanf("%d", &ram);
    printf("Enter the age of Shyam: \n");
    scanf("%d", &shyam);
    printf("Enter the age of Ajay: \n");
    scanf("%d", &ajay);
    if(ram>shyam && ram>ajay)
    {
        printf("Ram is eldest in all\n");
        if(shyam>ajay)
        {
            printf("Shyam is the middle of all\n");
            printf("Ajay is the youngest of all\n");
        }
        else
        {
            printf("Ajay is middle in all\n");
            printf("Shyam is ypungest of all\n");
        }
    }
    if(ajay>ram && ajay>shyam)
    {
        printf("Ajay is the eldest of all\n");
        if(ram>shyam)
        {
            printf("Ram is middle of all\n");
            printf("Shaym is youngest of all\n");
        }
        else
        {
            printf("Shyam is middle of all\n");
            printf("Ram is youngest of all\n");
        }
    }
    if(shyam>ram && shyam>ajay)
    {
        printf("Shyam is eldest of all\n");
        if(ram>ajay)
        {
            printf("Ram is middle of all\n");
            printf("Ajay is youngest of all\n");
        }
        else
        {
            printf("Ajay is middle of all\n");
            printf("Ram is youngest of all\n");
        }
    }

    return 0;
}
