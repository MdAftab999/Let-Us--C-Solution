#include<stdio.h>

int main(){
float l,b,p,a_r,r,c,a_c;
printf("Enter the lenght of ractangle");
scanf("%f",&l);
printf("Enter the breath of ractangle");
scanf("%f",&b);
printf("Enter the radius of circle");
scanf("%f",&r);
p = 2*(l+b);
a_r = l*b;
c = 2*3.14*r;
a_c = 3.14*r*r;
printf("The area of ractangle is :%f",a_r);
printf("The perimeter of ractangle is : %f",p);
printf("The circumference of circle is : %f", c);
printf("The area of circle is : %f",a_c);
return 0 ;
}
