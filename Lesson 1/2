#include<stdio.h>
#include<stdlib.h>
#include<math.h>
int main()
{
float cen_x,cen_y,rad,x,y,d;
printf("\n Enter the center x,y coordinate and radius:");
scanf("%f,%f,%f",&cen_x,&cen_y,&rad);
printf("\n Enter the coordinates of point x,y:");
scanf("%f,%f", &x,&y);
d=sqrt(pow((cen_x-x),2.0)+pow((cen_y-y),2.0));
if(d<rad)
{
    printf("point is inside the circle\n");
}
else if(d==rad)
{
    printf("point is on the circle\n");
}
else
printf(" the points is outside of circle\n");
     return 0;
}
