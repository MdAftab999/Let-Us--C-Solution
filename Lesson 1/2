#include<stdio.h>
int main(){
    int year, normal_days, leap_yer_days, total_days, odd_days;
    printf("Enter the yeear: ");
    scanf("%d",&year);
    normal_days = (year-1)*365;
    leap_yer_days = (year-1)/4+(year-1)/400-(year-1)/100;
    total_days = leap_yer_days+normal_days;
    odd_days = total_days%7;
    if(odd_days==0)
    {
    printf("Monday\n");
    }  
      if(odd_days==1)
    {
    printf("Tuesday\n");
    }

   if(odd_days==2)
   {
 
    printf("Wednesday\n");
    }  
     if(odd_days==3)
    {
    printf("Thursday\n");
    }  
       if(odd_days==4)
    {
    printf("Friday\n");
    }   
    if(odd_days==5)
    {
    printf("Saturday\n");
    }
         if(odd_days==6)
    {
    printf("Sunday\n");
    }
    return 0;
    }  
e
#include<stdio.h>
int main(){
    int number,a,b,c,d,e, reverse_number;
    printf("Enter the 5 digits number: ");
    scanf("%d", &number);
a= number/10000;
b=(number-a*10000)/1000;
c=(number-a*10000-b*1000)/100;
d=(number-a*10000-b*1000-c*100)/10;
e=number%10;
printf("The digits are : %d\n",a);
printf("The digits are : %d\n",b);
printf("The digits are : %d\n",c);
printf("The digits are : %d\n",d);
printf("The digits are : %d\n",e);
printf("The reverse number %d is %d%d%d%d%d\n", number,e,d,c,b,a);
reverse_number = e*10000+d*1000+c*100+b*10+a;
printf("The reverse of number is:  %d\n", reverse_number);
if(number==reverse_number)
{
    printf("The number is same number\n");
}
else
{
    printf("The reverse number is not same number\n");

}
 return 0;
}
