#include<stdio.h>
int main()
{
float x,y;
printf("\n Enter the points(x,y) coodinates");
scanf("%f%f",&x,&y);
if(x==0 && y==0)
{
printf("\nThe points lies on origin");
}
if(x==0 && y!=0)
{
printf(" \nthe point lies on y axis");
}
if(x!=0 && y==0)
{
printf("\n the point lies on x axis");
}
if(x!=0 && y!=0)
{
printf(" \nthe points lies on plane");
} 
    return 0;
}
