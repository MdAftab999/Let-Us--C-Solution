#include<stdio.h>
int main()
{
    float hardness, carbon , tensile;
    printf(" Enter steel :\n");
    scanf("%f %f %f",&hardness, &carbon, &tensile);
    if(hardness>50 && carbon<0.7 && tensile>5600)
    
        printf("Grade is 10 :\n");
    
    else if(hardness>50 && carbon<0.7 && tensile<=5600)
    
        printf("Grade 9:\n");
    
    else if(hardness<=50 && carbon<0.7 && tensile>5600)
    
        printf("Grade 8\n:");
    
    else if(hardness>50 && carbon>0.7 && tensile>5600)
    
        printf("Grade 7:\n"); 
    else if(hardness>50 || carbon<0.7 || tensile>5600)
       printf("Grade 6:\n");
    else
    printf("Grade 5\n");   
        return 0;
}
